<Keyboard xmlns:android="http://schemas.android.com/apk/res/android"
    android:keyWidth="16%p"
    android:horizontalGap="@dimen/hor_gap"
    android:verticalGap="1px"
    android:keyHeight="@dimen/key_height"
    >

    <Row>
        <Key android:codes="98" android:keyLabel="b" android:popupCharacters="1" android:popupKeyboard="@xml/keyboard_popup_template" android:keyEdgeFlags="left"/>
        <Key android:codes="107" android:keyLabel="k" android:popupCharacters="2" android:popupKeyboard="@xml/keyboard_popup_template" />
        <Key android:codes="100" android:keyLabel="d" android:popupCharacters="3" android:popupKeyboard="@xml/keyboard_popup_template"/>
        <Key android:codes="103" android:keyLabel="g" android:popupCharacters="4" android:popupKeyboard="@xml/keyboard_popup_template"/>
        <Key android:codes="-1" android:keyLabel="SHFT" />
        <Key android:codes="-5" android:keyLabel="DEL" 
                android:isRepeatable="true"
                android:keyEdgeFlags="right"/>
    </Row>
    <Row>
        <Key android:codes="99" android:keyLabel="c" android:popupCharacters="5" android:popupKeyboard="@xml/keyboard_popup_template" android:keyEdgeFlags="left"/>
        <Key android:codes="97" android:keyLabel="a" android:popupCharacters="6" android:popupKeyboard="@xml/keyboard_popup_template"/>
        <Key android:codes="110" android:keyLabel="n" android:popupCharacters="7" android:popupKeyboard="@xml/keyboard_popup_template"/>
        <Key android:codes="105" android:keyLabel="i" android:popupCharacters="8" android:popupKeyboard="@xml/keyboard_popup_template"/>
        <Key android:codes="109" android:keyLabel="m" android:popupCharacters="9" android:popupKeyboard="@xml/keyboard_popup_template"/>
        <Key android:codes="113" android:keyLabel="q" android:popupCharacters="0" android:popupKeyboard="@xml/keyboard_popup_template" android:keyEdgeFlags="right"/>

    </Row>
    <Row>
        <Key android:codes="102" android:keyLabel="f" android:keyEdgeFlags="left"/>
        <Key android:codes="108" android:keyLabel="l"/>
        <Key android:codes="101" android:keyLabel="e"/>
        <Key android:codes="115" android:keyLabel="s"/>
        <Key android:codes="121" android:keyLabel="y"/>
        <Key android:codes="120" android:keyLabel="x" android:keyEdgeFlags="right"/>
    </Row>
    <Row>
        <Key android:codes="106" android:keyLabel="j" android:keyEdgeFlags="left"/>
        <Key android:codes="104" android:keyLabel="h"/>
        <Key android:codes="116" android:keyLabel="t"/>
        <Key android:codes="111" android:keyLabel="o"/>
        <Key android:codes="112" android:keyLabel="p"/>
        <Key android:codes="118" android:keyLabel="v" android:keyEdgeFlags="right"/>
    </Row>
    <Row>
    	<Key android:codes="44" android:keyLabel="," android:keyEdgeFlags="left"
    		android:popupCharacters="!?;:'\u0022" android:popupKeyboard="@xml/keyboard_popup_template"/>
        <Key android:codes="46" android:keyLabel="."
        	android:popupCharacters="+-@#\u0026*" android:popupKeyboard="@xml/keyboard_popup_template"/>
        <Key android:codes="114" android:keyLabel="r"/>
        <Key android:codes="117" android:keyLabel="u"/>
        <Key android:codes="119" android:keyLabel="w"/>
        <Key android:codes="122" android:keyLabel="z" android:keyEdgeFlags="right"/>
    </Row>
    <Row android:rowEdgeFlags="bottom">
    	<Key android:codes="-2" android:keyLabel="SYM"
                android:keyEdgeFlags="left" android:keyWidth="20%p"/>
<!--         <Key android:codes="-10" android:keyLabel="SYM"/>  -->
        <Key android:codes="32" android:keyIcon="@drawable/sym_keyboard_space" 
                android:isRepeatable="true" android:keyWidth="57.25%p"/> 
        <Key android:codes="10" android:keyIcon="@drawable/sym_keyboard_return" 
                android:keyEdgeFlags="right" android:keyWidth="20%p"/>
    </Row>
    <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              android:layout_width="fill_parent" 
              android:layout_height="fill_parent" 
              android:orientation="horizontal" >
              <LinearLayout
				    android:orientation="vertical"
				    android:layout_width="fill_parent"
				    android:layout_height="fill_parent"
				    android:layout_weight="100"
				    android:background="#00aa00">
				    <TextView
				        android:text="row one"
				        android:textSize="15pt"
				        android:layout_width="fill_parent"
				        android:layout_height="wrap_content"
				        android:layout_weight="1"/>
			</LinearLayout>
	</LinearLayout>
</Keyboard>

    